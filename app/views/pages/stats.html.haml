= javascript_include_tag "//www.google.com/jsapi", "chartkick"

= render 'layouts/top'
= render 'layouts/header'

.container
  %h1 Roadmaps registrados
  = User.without_yopolt.count
  
  %h2 Países
  =  geo_chart user_countries_path
  
  %h2 Ámbitos de trabajo
  = pie_chart(@user_ambitos)
  %h2 Sectores de empresa
  = pie_chart(@user_sectores)
  %h2 Número de integrantes de equipo
  = pie_chart(User.get_size_team_stats)
 

  %h2 Posición media de cada objetivo
  = column_chart(@objetivos_position)

  %h2 Posición media de cada práctica
  = column_chart(@pracs_position)
  
  %h2 Nivel medio de aplicación de cada práctica
  = column_chart(@margen_stats)

 

